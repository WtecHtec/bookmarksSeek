{
  "name": "BookmarksHelper",
  "displayName": "Bookmarks Helper",
  "version": "0.0.1",
  "description": "A launcher for managing browser bookmarks and easy retrieval.",
  "author": "r7. <504105925@qq.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.10.0",
    "cmdk": "^1.0.0",
    "framer-motion": "^11.1.9",
    "plasmo": "0.86.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "bookmarks",
      "storage",
      "unlimitedStorage"
    ],
    "commands": {
      "active_extention_launcher": {
        "suggested_key": {
          "default": "Ctrl+H",
          "mac": "Command+H"
        },
        "description": "Active Extension Launcher"
      }
    }
  }
}
